<tool id="fa_gc_content_1" name="First Tool for Test" version="0.1.0">
  <description>for each sequence in a file</description>
  <command>java -jar ${JAR_PATH}/test1.jar $input $output</command>
  <inputs>
    <param name="input" type="data" label="Source file"/>
  </inputs>
  <outputs>
    <data  name="output" />
  </outputs>

  <tests>
    <test>
      <param name="input" value="fa_gc_content_input.fa"/>
      <output name="out_file1" file="fa_gc_content_output.txt"/>
    </test>
  </tests>

  <requirements>
    <requirement type="set_environment">JAR_PATH</requirement>
  </requirements>

</tool>
