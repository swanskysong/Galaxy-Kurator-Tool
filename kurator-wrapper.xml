<tool id="kurator_1" name="Kurator-wrapper" version="0.1.0">
  <description>The wrapper to run Kurator in Galaxy</description>
  <command>java -jar \$JAR_PATH/kurator-akka.jar -f classpath:/org/kurator/akka/samples/hello.yaml</command>
  <inputs>
    <param name="input" type="data" label="Source file"/>
  </inputs>
  <outputs>
    <data  name="output" />
  </outputs>
  <requirements>
    <requirement type="package" version="0.0.1">kurator_package</requirement>
  </requirements>
</tool>
